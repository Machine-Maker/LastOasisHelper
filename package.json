{
  "name": "nomads-notebook",
  "version": "1.0.0",
  "description": "A webapp for Last Oasis",
  "author": "Machine Maker",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "cross-env NODE_ENV=development nuxt build",
    "start": "cross-env node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "stage": "git push staging HEAD:master",
    "prod": "git push production HEAD:master"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "body-parser": "^1.19.0",
    "cookie-universal-nuxt": "^2.1.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "express-validator": "^6.4.0",
    "nuxt": "^2.0.0",
    "nuxt-leaflet": "^0.0.21",
    "pg": "^8.0.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1"
  }
}
